{

  "manifest_version": 2,
  "name": "99lookup",
  "version": "1.0",

  "description": "Matchvote lookup for 99damage.",

  "icons": {
    "48": "icons/table.svg"
  },

  "content_scripts": [
    {
      "matches": ["*://liga.99damage.de/de/leagues/teams/*"],
      "js": ["99lookup.js"]
    }
  ],

  "permissions": [
    "webRequest",
    "<all_urls>"
  ]

}